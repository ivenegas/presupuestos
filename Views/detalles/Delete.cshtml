@model espaciodiseno.Models.detalles

@{
    ViewBag.Title = "Delete";
}

<h3>¿Esta seguro de eliminar este detalle de materiales?</h3>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            Detalle
        </dt>

        <dd>
            @Html.DisplayFor(model => model.det_nombre)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            <input type="button" value="Cancelar" class="btn btn-warning" onclick="location.href='@Url.Action("Index", "detalles" )'" />
        </div>
    }
</div>
